Algoritmo "semnome"

Var

 nome : caractere
 idade : inteiro

Inicio

 //Entrada de dados
 escreva("Digite seu nome: ")
 leia(nome)

 escreva("Digite sua idade: ")
 leia(idade)
 
 //Saida de dados
 
 escreval
 
 escreval("Nome: ", nome)
 escreval("Idade: ", idade, " anos")


Fimalgoritmo